locations:
  - name: "{{ site_name }}"
    status__name: "Staging"
    location_type__name: "Site"
    parent__name: "{{ region.name }}"
    physical_address: "789 Mega Avenue, Server Street"
    latitude: 39.1234
    longitude: -124.5678

racks:
  - name: "Rack-01"
    location__name: "{{ site_name }}"
    status__name: "Available"
    u_height: 42
    type: ""

cluster_types:
  - "!create_or_update:name": "Generic"
    "!ref": "cluster_type"

clusters:
  - name: "{{ site_name }}-cluster"
    location__name: "!ref:my_location"
    cluster_type: "!ref:cluster_type"
    "!ref": "mycluster"

virtual_machines:
