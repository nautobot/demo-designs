    status__name: "Active"
    cluster: "!ref:mycluster"
    tenant__name: "Finance Team"
    vcpus: 4
    memory: {{ memory_per_vm|int * 1024 }}
    disk: 80
    interfaces:
      - name: "eth0"
        status__name: "Active"
